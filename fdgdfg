class Solution:
    def maximumSum(self, nums: List[int]) -> int:
        def dsum(num):
            total = 0
            while num > 0:
                total += num % 10
                num = num // 10
            return total

        digit_sum = collections.defaultdict(list)
        for num in nums:
            dtotal = dsum(num)
            digit_sum[dtotal].append(num)
        res = -1
        
        for k, v in digit_sum.items():
            if len(v) >= 2:
                v.sort() 
                res = max(res, v[-1] + v[-2])
        return res
        
